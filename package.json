    "jshint": "^2.6.3",
    "mocha": "~2",
    "plato": "^1.4.0",
    "should": "~4",
    "sinon": "~1",
    "ws": "0.5.x"
  },
  "dependencies": {
    "events": "~1",
    "underscore": "^1.8.2"
  },
  "config": {
    "blanket": {
      "pattern": "/barrel\/lib/"

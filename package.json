{
  "author": "Zach Rose",
  "name": "barrel",
  "version": "0.1.0",
  "description": "An abstract player for sequences of timed events.",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "cov": "mocha --require blanket -R html-cov > coverage.html",
